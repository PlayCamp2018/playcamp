# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~

# An example controller showing a sample home page
GET     /                           controllers.HomeController.index

# Map static resources from the /public folder to the /assets URL path
GET     /assets/*file               controllers.Assets.versioned(path="/public", file: Asset)

GET     /listUsers                  controllers.DBTestController.listUser

GET     /login                      controllers.HomeController.login
GET     /lang                       controllers.TestLangController.index

GET     /TestFileUpload             controllers.TestFileUploadController.index

+ nocsrf
POST    /TestFileUpload             controllers.TestFileUploadController.upload

POST    /upload                     controllers.TestFileUploadController.upload

GET     /users                      controllers.UserController.index
GET     /getUser                    controllers.UserController.getUser
GET     /user/get/:id               controllers.UserController.get(id: Int)

GET     /projects                   controllers.ProjectController.index
GET     /project/get/:id            controllers.ProjectController.get(id: Int)